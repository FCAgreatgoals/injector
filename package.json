{
  "name": "@fca.gg/injector",
  "version": "1.2.5",
  "description": "CID Library for Node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib/",
    "dev": "npm run clean && tsc && concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon --inspect lib/index.js\"",
    "build": "npm run clean && tsc",
    "prod-build": "tsc",
    "start": "node lib/index.js"
  },
  "dependencies": {
    "joi": "^17.9.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FCAgreatgoals/injector.git",
